{
    "type": "minecraft:multi_action",
    "title": {
        "translate": "dialog.aspects.configs.entry",
        "fallback": "!! Missing Resource Pack for Aspects !!",
        "with": [
            {
                "translate": "reclaimed",
                "fallback": "!! Missing Resource Pack for Aspects: Reclaimed !!",
                "color": "blue"
            },
            {
                "translate": "config.reclaimed.voidborne.storage_hp_threshold",
                "fallback": "!! Missing Resource Pack for Aspects: Reclaimed !!"
            }
        ]
    },
    "external_title": {
        "translate": "dialog.aspects.configs.entry.external_title",
        "fallback": "!! Missing Resource Pack for Aspects !!",
        "with": [
            {
                "translate": "reclaimed",
                "fallback": "!! Missing Resource Pack for Aspects: Reclaimed !!",
                "color": "blue"
            },
            {
                "translate": "config.reclaimed.voidborne.storage_hp_threshold",
                "fallback": "!! Missing Resource Pack for Aspects: Reclaimed !!"
            }
        ]
    },
    "body": [
        {
            "type": "minecraft:plain_message",
            "width": 450,
            "contents": {
                "translate": "config.reclaimed.voidborne.storage_hp_threshold.description",
                "extra": [
                    "\n",
                    {
                        "translate": "config.reclaimed.voidborne.storage_hp_threshold.note",
                        "color": "#d6d6d6"
                    }
                ]
            }
        },
        {
            "type": "minecraft:plain_message",
            "width": 450,
            "contents": {
                "text": "",
                "extra": [
                    {
                        "translate": "dialog.aspects.configs.entry.accepts_values",
                        "with": [
                            {
                                "text": "",
                                "color": "#d6d6d6",
                                "extra": [
                                    {
                                        "translate": "dialog.aspects.configs.entry.accepts_values.int",
                                        "color": "blue"
                                    }
                                ]
                            }
                        ]
                    },
                    "\n",
                    {
                        "translate": "dialog.aspects.configs.entry.default",
                        "with": [
                            {
                                "text": "40",
                                "color": "blue"
                            }
                        ]
                    }
                ]
            }
        }
    ],
    "inputs": [
        {
            "type": "minecraft:number_range",
            "label": {
                "translate": "dialog.aspects.configs.entry.change_value"
            },
            "key": "value",
            "width": 225,
            "start": -1,
            "end": 512,
            "initial": 40,
            "step": 1
        }
    ],
    "can_close_with_escape": true,
    "pause": true,
    "exit_action": {
        "label": {
            "translate": "gui.back"
        },
        "action": {
            "type": "minecraft:show_dialog",
            "dialog": "aspects:menu/configs"
        }
    },
    "columns": 2,
    "actions": [
        {
            "label": {
                "translate": "dialog.aspects.configs.entry.apply_input_value"
            },
            "action": {
                "type": "minecraft:dynamic/run_command",
                "template": "scoreboard players set #reclaimed reclaimed.config.voidborne.storage_hp_threshold $(value)"
            }
        },
        {
            "label": {
                "translate": "dialog.aspects.configs.entry.reset_value"
            },
            "action": {
                "type": "minecraft:run_command",
                "command": "function reclaimed:config/voidborne/storage_hp_threshold/default"
            }
        },
        {
            "label": {
                "translate": "dialog.aspects.configs.entry.check_value"
            },
            "action": {
                "type": "minecraft:run_command",
                "command": "function reclaimed:config/voidborne/storage_hp_threshold/current"
            }
        }
    ]
}