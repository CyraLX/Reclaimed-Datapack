# Create and set bossbar with ID
$bossbar add reclaimed:voidborne/store_entity/player_$(id) ""
$bossbar set reclaimed:voidborne/store_entity/player_$(id) players @s
$bossbar set reclaimed:voidborne/store_entity/player_$(id) color purple
$bossbar set reclaimed:voidborne/store_entity/player_$(id) visible false

